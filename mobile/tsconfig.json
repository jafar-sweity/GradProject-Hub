{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "hooks/useAuthCheck.jsx",
    "hooks/useFetchData.jsx",
    "hooks/useFetchData.jsx",
    "app/(tabs)/explore.jsx"
  ]
}
